<p id="notice"><%= notice %></p>
  <div><h1 class="text-center p-title"></span> 生徒作品一覧</h1></div>
  


<div class='row row-height'>
    <% @completes.each do |complete| %>
      <div class="col-xs-6 col-sm-4 col-md-3">
        <div class="panel panel-primary">
          <div class="panel-body text-center">
            <h4> 作品名：<%= complete.name %></h4>
            <p>開発者：<%#= User.find_by(name: complete.user_name).nickname %></p>
            <p><%= image_tag complete.img_path %></p>
            <p><%= complete.description %></p>
            <%= link_to '表示する', complete, target: "_brank", class: "btn btn-default", role: "button"%>
          </div>
        </div>
      </div>
    <% end %>
</div>
